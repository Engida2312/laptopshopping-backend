    <%- include('includes/admin-header') %>
    
    <div class="admin_body_container">
        <h1>All Users</h1>
        <div class="user_table_container">
            <table>
            
                <thead>
                    <th>Profile</th>
                    <th>Name</th>
                    <th>E-mail</th>
                <th>Status</th>
                    <th class="address_col">Address</th>
                <th>Edit</th>
                
                
                </thead>
                <tbody>
                    <%
                    users.forEach((user)=>{%>
                        <tr>
                            <td><img src="./yohannes.JPG" alt="no image"></td>
                            <td><%=user.username%></td>
                            <td id="email"><%=user.email%></td>
                            <td><%=user.role%></td>
]                            <td ><i class="fa fa-caret-right"></i></td>
                        
                        </tr>
                    <%})
                    %>
                </tbody>
                
                
            </table>
        </div>
    </div>
    
  
</body>
